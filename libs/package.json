{
  "name": "@fom/shared",
  "description": "FOM Shared Library",
  "version": "0.0.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test-unit": "set LOG_LEVEL=warn && jest"
  },
  "dependencies": {
    "@angular/common": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@nestjs/common": "^11.1.3",
    "@nestjs/core": "^11.1.3",
    "@nestjs/testing": "^11.1.3",
    "leaflet": "~1.9.0",
    "leaflet.markercluster": "~1.5.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/leaflet": "~1.9.0",
    "@types/leaflet.markercluster": "~1.5.1",
    "jest": "^30.0.0",
    "jest-sonar-reporter": "^2.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "~9.1.1",
    "typescript": "~5.1.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "utility/src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "cobertura"
    ],
    "testResultsProcessor": "jest-sonar-reporter"
  }
}
