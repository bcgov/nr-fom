    "swagger-ui-express": "^4.1.6",
    "tslib": "^2.0.0",
    "tsutils": "^3.21.0",
    "typeorm": "^0.2.32",
    "typeorm-naming-strategies": "^2.0.0",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1102.14",
    "@angular/cli": "~11.2.14",
    "@angular/compiler-cli": "~11.2.14",
    "@angular/language-service": "~11.2.14",
    "@nestjs/cli": "^7.5.6",
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/testing": "^7.6.15",
    "@nrwl/cli": "11.2.0",
    "@nrwl/cypress": "11.6.3",
    "@nrwl/eslint-plugin-nx": "11.2.0",
    "@nrwl/jest": "11.2.0",
    "@nrwl/linter": "11.2.0",
    "@nrwl/nest": "11.2.0",
    "@nrwl/node": "11.2.0",
    "@nrwl/tao": "11.2.0",
    "@nrwl/workspace": "11.2.0",
    "@openapitools/openapi-generator-cli": "^2.1.26",
    "@types/cron": "^1.7.2",
    "@types/express-session": "^1.17.3",
    "@types/faker": "^5.1.7",
    "@types/geojson": "^7946.0.7",
    "@types/jest": "^26.0.21",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/leaflet": "^1.7.0",
    "@types/leaflet.markercluster": "^1.4.0",
    "@types/lodash": "^4.14.170",
