    "eslint": "7.10.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-cypress": "^2.10.3",
    "jest": "26.2.2",
    "jest-preset-angular": "8.3.2",
    "prettier": "2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "26.4.0",
    "ts-node": "~9.1.1",
    "ts-toolbelt": "^9.6.0",
    "typeorm-fixtures-cli": "^1.9.1",
    "typescript": "^4.1.5"
  }
}
