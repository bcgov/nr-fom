{
  "name": "fom-api",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start:api": "nest start --path apps/api/tsconfig.json --watch",
    "start:batch": "nest start --path apps/api/tsconfig.json -- -batch",
    "start:public": "nx run public:serve --port=4200",
    "start:admin": "nx run admin:serve --port=4300",
    "build:api": "nx run api:build",
    "build:public": "nx run public:build",
    "build:admin": "nx run admin:build",
    "gen:client-api:ng": "openapi-generator-cli generate -g typescript-angular -i apps/api/openapi/swagger-spec.json -o libs/client/typescript-ng",
    "debug:api": "nodemon --config nodemon.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "db:migrate-main": "npm run typeorm migration:run -- -f ./apps/api/src/migrations/ormconfig-migration-main.ts",
    "db:migrate-main:revert": "npm run typeorm migration:revert -- -f ./apps/api/src/migrations/ormconfig-migration-main.ts",
    "db:migrate-main:show": "npm run typeorm migration:show -- -f ./apps/api/src/migrations/ormconfig-migration-main.ts",
    "db:migrate-test": "npm run typeorm migration:run -- -f ./apps/api/src/migrations/ormconfig-migration-test.ts",
    "db:migrate-test:revert": "npm run typeorm migration:revert -- -f ./apps/api/src/migrations/ormconfig-migration-test.ts",
    "db:migrate-test:show": "npm run typeorm migration:show -- -f ./apps/api/src/migrations/ormconfig-migration-test.ts",
    "db:recreate": "docker-compose down && docker-compose up -d db && npm run db:migrate-main && npm run db:migrate-test",
    "db:start": "docker-compose up -d db",
    "db:stop": "docker-compose stop db",
    "perf-test:gen-data": "node apps/api/src/perftest/generate-data.js",
    "test-ci": "nx run api:test --testFile='/((?!e2e).)*$'",
    "test-unit": "set LOG_LEVEL=warn&&nx run api:test --testFile='/((?!e2e).)*$'",
    "test-unit-watch": "set LOG_LEVEL=warn&&nx run api:test --testFile='/((?!e2e).)*$' -- --watch=true",
    "test-e2e": "set LOG_LEVEL=warn&&nx run api:test --testFile='e2e.spec.ts$'",
    "build": "nx build",
    "test": "nx test",
    "lint": "nx workspace-lint && nx lint",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "disable-postinstall": "ngcc --properties es2015 browser module main",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.14",
    "@angular/cdk": "^11.2.9",
    "@angular/common": "^11.2.14",
    "@angular/compiler": "^11.2.14",
    "@angular/core": "^11.2.14",
    "@angular/forms": "^11.2.14",
    "@angular/material": "^11.2.9",
    "@angular/platform-browser": "^11.2.14",
    "@angular/platform-browser-dynamic": "^11.2.14",
    "@angular/router": "^11.2.14",
    "@nestjs-modules/mailer": "^1.6.0",
    "@nestjs/common": "^7.6.15",
