{
  "name": "fom-api",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start:api": "nest start --path apps/api/tsconfig.json --watch",
    "start:batch": "nest start --path apps/api/tsconfig.json -- -batch",
    "start:public": "nx run public:serve --port=4300 --host=0.0.0.0",
    "start:admin": "nx run admin:serve --port=4200 --host=0.0.0.0",
    "build:api": "nx run api:build",
    "build:public": "nx run public:build",
    "build:admin": "nx run admin:build",
    "gen:client-api:ng": "openapi-generator-cli generate -g typescript-angular -i api/openapi/swagger-spec.json -o libs/client/typescript-ng",
    "debug:api": "nodemon --config nodemon.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "db:migrate-main": "npm run typeorm migration:run -- -f ./api/src/migrations/ormconfig-migration-main.ts",
    "db:migrate-main:revert": "npm run typeorm migration:revert -- -f ./api/src/migrations/ormconfig-migration-main.ts",
    "db:migrate-main:show": "npm run typeorm migration:show -- -f ./api/src/migrations/ormconfig-migration-main.ts",
    "db:migrate-test": "npm run typeorm migration:run -- -f ./api/src/migrations/ormconfig-migration-test.ts",
    "db:migrate-test:revert": "npm run typeorm migration:revert -- -f ./api/src/migrations/ormconfig-migration-test.ts",
    "db:migrate-test:show": "npm run typeorm migration:show -- -f ./api/src/migrations/ormconfig-migration-test.ts",
    "db:recreate": "docker-compose down && docker-compose up -d db && cd api && npm run db:migrate-main && npm run db:migrate-test",
    "db:start": "docker-compose up -d db",
    "db:stop": "docker-compose stop db",
    "perf-test:gen-data": "node apps/api/src/perftest/generate-data.js",
    "test-ci": "nx run api:test --testFile='/((?!e2e).)*$'",
    "test-unit": "set LOG_LEVEL=warn&&nx run api:test --testFile='/((?!e2e).)*$'",
    "test-unit-watch": "set LOG_LEVEL=warn&&nx run api:test --testFile='/((?!e2e).)*$' -- --watch=true",
    "test-e2e": "set LOG_LEVEL=warn&&nx run api:test --testFile='e2e.spec.ts$'",
    "build": "nx build",
    "test": "nx test",
    "lint": "nx workspace-lint && nx lint",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "disable-postinstall": "ngcc --properties es2015 browser module main",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.14",
    "@angular/cdk": "^11.2.13",
    "@angular/common": "^11.2.14",
    "@angular/compiler": "^11.2.14",
    "@angular/core": "^11.2.14",
    "@angular/forms": "^11.2.14",
    "@angular/material": "^11.2.13",
    "@angular/platform-browser": "^11.2.14",
    "@angular/platform-browser-dynamic": "^11.2.14",
    "@angular/router": "^11.2.14",
    "@auth0/angular-jwt": "^5.0.2",
    "@fortawesome/angular-fontawesome": "^0.8.2",
    "@fortawesome/fontawesome-svg-core": "^1.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.0.0",
    "@nestjs-modules/mailer": "^1.6.1",
    "@nestjs/common": "^7.6.18",
    "@nestjs/config": "^1.2.0",
    "@nestjs/core": "^7.6.18",
    "@nestjs/platform-express": "^7.6.18",
    "@nestjs/schedule": "^1.0.2",
    "@nestjs/swagger": "^4.8.2",
    "@nestjs/typeorm": "^7.1.5",
    "@ng-bootstrap/ng-bootstrap": "^9.1.3",
    "@rxweb/reactive-form-validators": "^2.1.6",
    "bootstrap": "^4.5.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dayjs": "^1.10.8",
    "dotenv": "^16.0.0",
    "helmet": "^5.0.2",
    "joi": "^17.6.0",
    "jquery": "^3.5.1",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "2.0.3",
    "keycloak-js": "^12.0.4",
    "leaflet": "~1.7.1",
    "leaflet.markercluster": "~1.5.3",
    "lodash": "^4.17.21",
    "minio": "^7.0.26",
    "moment": "^2.29.2",
    "nestjs-pino": "^1.4.0",
    "ngx-bootstrap": "^7.1.2",
    "ngx-dropzone": "^3.0.0",
    "node-cache": "^5.1.2",
    "object-hash": "^3.0.0",
    "pg": "^8.5.1",
    "popper.js": "^1.16.1",
    "reflect-metadata": "^0.1.13",
    "remeda": "^0.0.32",
    "rxjs": "~7.5.5",
    "swagger-ui-express": "^4.3.0",
    "tslib": "^2.3.1",
    "tsutils": "^3.21.0",
    "typeorm": "^0.2.34",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.1102.18",
    "@angular-devkit/build-angular": "~0.1102.18",
    "@angular-devkit/core": "~11.2.18",
    "@angular/cli": "~11.2.18",
    "@angular/compiler-cli": "~11.2.14",
    "@nestjs/cli": "^7.6.0",
    "@nestjs/testing": "^7.6.18",
    "@nrwl/cli": "^11.6.3",
    "@nrwl/eslint-plugin-nx": "^11.6.3",
    "@nrwl/jest": "^11.6.3",
    "@nrwl/nest": "^11.6.3",
    "@nrwl/node": "^11.6.3",
    "@nrwl/workspace": "^11.6.3",
    "@openapitools/openapi-generator-cli": "^2.4.26",
    "@types/geojson": "^7946.0.8",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/leaflet": "~1.7.11",
    "@types/leaflet.markercluster": "~1.5.1",
    "@types/lodash": "^4.14.179",
    "@types/multer": "^1.4.7",
    "jest": "^27.5.1",
    "jest-preset-angular": "^11.1.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "~9.1.1",
    "typescript": "~4.1.6"
  }
}
