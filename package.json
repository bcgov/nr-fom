{
  "name": "fom-api",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "start": "nx serve",
    "start:debug": "nodemon --config nodemon.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "db:migrate-main": "npm run typeorm migration:run -- -f ./apps/api/src/migrations/ormconfig-migration-main.ts",
    "db:migrate-main:revert": "npm run typeorm migration:revert -- -f ./apps/api/src/migrations/ormconfig-migration-main.ts",
    "db:migrate-main:show": "npm run typeorm migration:show -- -f ./apps/api/src/migrations/ormconfig-migration-main.ts",
    "db:migrate-test": "npm run typeorm migration:run -- -f ./apps/api/src/migrations/ormconfig-migration-test.ts",
    "db:migrate-test:revert": "npm run typeorm migration:revert -- -f ./apps/api/src/migrations/ormconfig-migration-test.ts",
    "db:migrate-test:show": "npm run typeorm migration:show -- -f ./apps/api/src/migrations/ormconfig-migration-test.ts",
    "build": "nx build",
    "test": "nx test",
    "lint": "nx workspace-lint && nx lint",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "init:dev": "docker-compose -p nr-fom-api up --build -V",
    "start:api": "nest start --path apps/api/tsconfig.json --watch",
    "start:db:postgres": "docker-compose up db",
    "stop:db:postgres": "docker-compose stop db",
    "gen:client-api:ng": "openapi-generator-cli generate -g typescript-angular -i apps/api/openapi/swagger-spec.json -o libs/client/typescript-ng && npm run format:write",
    "gen:client-api:rxjs": "npm run gen:fetch-template:rxjs && npm run gen:models:rxjs && npm run format:write",
    "gen:client-api:axios": "npm run gen:fetch-template:axios && npm run gen:models:axios && npm run format:write",
    "gen:models:rxjs": "openapi-generator-cli generate -g typescript-rxjs -t apps/api/openapi/template/typescript-rxjs -i apps/api/openapi/swagger-spec.json -o libs/client/typescript-rxjs",
    "gen:models:axios": "openapi-generator-cli generate -g typescript-axios -t apps/api/openapi/template/typescript-axios -i apps/api/openapi/swagger-spec.json -o libs/client/typescript-axios",
    "gen:fetch-template:rxjs": "bash ./apps/api/openapi/.fetch-template-typescript-rxjs",
    "gen:fetch-template:axios": "bash ./apps/api/openapi/.fetch-template-typescript-axios"
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "^7.6.15",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.15",
    "@nestjs/mapped-types": "^0.3.0",
    "@nestjs/microservices": "^7.6.15",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/swagger": "^4.8.0",
    "@nestjs/typeorm": "^7.1.5",
    "@types/faker": "^5.1.7",
    "@types/geojson": "^7946.0.7",
    "class-transformer": "^0.3.2",
    "class-validator": "^0.13.1",
    "dotenv": "^6.2.0",
    "express-session": "^1.17.1",
    "joi": "^17.4.0",
    "nestjs-pino": "^1.3.0",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "remeda": "0.0.27",
    "rxjs": "^6.6.6",
    "swagger-ui-express": "^4.1.6",
    "tslib": "^2.0.0",
    "tsutils": "^3.21.0",
    "typeorm": "^0.2.31",
    "typeorm-naming-strategies": "^2.0.0"
  },
  "devDependencies": {
    "@angular/common": "~11.2.9",
    "@angular/core": "~11.2.9",
    "@devniel/nestjs-typeorm-testing": "^0.1.1-alpha",
    "@nestjs/cli": "^7.5.6",
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/testing": "^7.6.15",
    "@nrwl/cli": "11.2.0",
    "@nrwl/eslint-plugin-nx": "11.2.0",
    "@nrwl/jest": "11.2.0",
    "@nrwl/linter": "10.4.4",
    "@nrwl/nest": "11.2.0",
    "@nrwl/node": "11.2.0",
    "@nrwl/react": "^11.5.2",
    "@nrwl/react-native": "^11.4.1",
    "@nrwl/tao": "11.2.0",
    "@nrwl/workspace": "11.2.0",
    "@openapitools/openapi-generator-cli": "^2.1.26",
    "@testing-library/jest-native": "3.4.3",
    "@types/express-session": "^1.17.3",
    "@types/jest": "^26.0.21",
    "@types/node": "^12.20.6",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "eslint": "7.10.0",
    "eslint-config-prettier": "6.0.0",
    "jest": "26.2.2",
    "jest-react-native": "18.0.0",
    "metro-react-native-babel-preset": "0.63.0",
    "prettier": "2.2.1",
    "react-test-renderer": "17.0.1",
    "supertest": "^6.1.3",
    "ts-jest": "26.4.0",
    "ts-node": "~9.1.1",
    "ts-toolbelt": "^9.6.0",
    "typeorm-fixtures-cli": "^1.9.1",
    "typescript": "^4.0.7"
  }
}
